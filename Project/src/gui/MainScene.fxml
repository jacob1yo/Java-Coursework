<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <right>
      <VBox prefHeight="600.0" prefWidth="250.0" style="-fx-border-color: black;" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-border-color: black; -fx-background-color: lightgrey;">
               <children>
                  <Label style="-fx-font-weight: bold;" text="Warehouse">
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </Label>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Label text="Height">
                                 <HBox.margin>
                                    <Insets left="10.0" right="30.0" />
                                 </HBox.margin>
                              </Label>
                              <Slider />
                           </children>
                        </HBox>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Label text="Width">
                                 <HBox.margin>
                                    <Insets left="10.0" right="30.0" />
                                 </HBox.margin>
                              </Label>
                              <Slider />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-border-color: black; -fx-background-color: #939393;">
               <children>
                  <Label alignment="TOP_LEFT" style="-fx-font-weight: bold;" text="Orders" />
                  <TextArea prefHeight="200.0" prefWidth="200.0">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </TextArea>
                  <Button alignment="CENTER" mnemonicParsing="false" text="Load">
                     <VBox.margin>
                        <Insets bottom="10.0" left="150.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-border-color: black; -fx-background-color: lightgrey;">
               <children>
                  <Label style="-fx-font-weight: bold;" text="Robot">
                     <VBox.margin>
                        <Insets bottom="15.0" />
                     </VBox.margin>
                  </Label>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Battery Capacity">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" />
                           </VBox.margin>
                        </Label>
                        <Slider>
                           <VBox.margin>
                              <Insets left="10.0" />
                           </VBox.margin>
                        </Slider>
                     </children>
                  </VBox>
                  <VBox layoutX="11.0" layoutY="32.0" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Charge Rate">
                           <VBox.margin>
                              <Insets bottom="10.0" />
                           </VBox.margin>
                        </Label>
                        <Slider />
                     </children>
                     <VBox.margin>
                        <Insets left="10.0" />
                     </VBox.margin>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </VBox>
         </children>
      </VBox>
   </right>
   <top>
      <HBox alignment="CENTER" prefHeight="50.0" spacing="10.0" style="-fx-border-color: black;" BorderPane.alignment="CENTER">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" text="Robot">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <Button alignment="CENTER" mnemonicParsing="false" text="Storage" />
            <Button alignment="CENTER" mnemonicParsing="false" text="Packing" />
            <Button alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" text="Delete">
               <HBox.margin>
                  <Insets right="800.0" />
               </HBox.margin>
            </Button>
            <Button alignment="CENTER" mnemonicParsing="false" text="Clear " />
            <Button alignment="CENTER" mnemonicParsing="false" text="Save">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0" style="-fx-border-color: black;" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="1000.0">
               <children>
                  <Label text="Group 10" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" text="Button" />
               </children>
            </HBox>
         </children>
      </HBox>
   </bottom>
</BorderPane>
